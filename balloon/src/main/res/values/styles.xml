    <item name="android:windowEnterAnimation">@anim/show_up_center</item>
    <item name="android:windowExitAnimation">@anim/show_down_center</item>
  </style>

  <style name="NormalDispose">
    <item name="android:windowExitAnimation">@anim/show_down_center</item>
  </style>

  <style name="Elastic">
    <item name="android:windowEnterAnimation">@anim/elastic_center</item>
    <item name="android:windowExitAnimation">@anim/dispose_center</item>
  </style>

  <style name="Fade">
    <item name="android:windowEnterAnimation">@anim/fade_in</item>
    <item name="android:windowExitAnimation">@anim/fade_out</item>
  </style>
</resources>
